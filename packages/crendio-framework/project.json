{
  "name": "crendio-framework",
  "$schema": "..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "sourceRoot": "packages/crendio/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "node ./build.js",
        "cwd": "packages/crendio-framework"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/crendio-framework/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/crendio-framework"],
      "options": {
        "jestConfig": "packages/crendio-framework/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
